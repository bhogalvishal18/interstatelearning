<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginValidate</servlet-name>
        <servlet-class>com.bhogal.LoginValidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterValidate</servlet-name>
        <servlet-class>com.bhogal.RegisterValidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <jsp-file>/login.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <jsp-file>/register.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <jsp-file>/home.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>EditProfile</servlet-name>
        <jsp-file>/editprofile.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ViewProfile</servlet-name>
        <jsp-file>/viewprofile.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>AddBank</servlet-name>
        <jsp-file>/addbank.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ViewBank</servlet-name>
        <jsp-file>/viewbank.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <jsp-file>/logout.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ComingSoon</servlet-name>
        <jsp-file>/comingsoon.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>EditProfileDetail</servlet-name>
        <servlet-class>com.bhogal.EditProfileDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBankDetails</servlet-name>
        <servlet-class>com.bhogal.AddBankDetails</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginValidate</servlet-name>
        <url-pattern>/loginvalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterValidate</servlet-name>
        <url-pattern>/registervalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfile</servlet-name>
        <url-pattern>/editprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewProfile</servlet-name>
        <url-pattern>/viewprofile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBank</servlet-name>
        <url-pattern>/addbank</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewBank</servlet-name>
        <url-pattern>/viewbank</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ComingSoon</servlet-name>
        <url-pattern>/comingsoon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfileDetail</servlet-name>
        <url-pattern>/editprofiledetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBankDetails</servlet-name>
        <url-pattern>/addbankdetails</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <error-page>
        <location>/404page.html</location>
    </error-page>
</web-app>
